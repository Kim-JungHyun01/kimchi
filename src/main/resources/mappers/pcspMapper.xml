<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kr.kimchi.mappers.pcspMapper">
	
	<select id="pcspList" resultType="com.kr.kimchi.vo.PcspVO">
		 select * from pcsp 
		 where partner_taxid = #{partner_taxid}; 
	</select>
	
	<insert id="pcspInsert" >
		 insert into pcsp 
		 (partner_taxid, code_name, pcsp_status, pcsp_date, pcsp_notes) 
		 values (#{partner_taxid},#{code_name},0,#{pcsp_date},#{pcsp_notes});
	</insert>

	




</mapper>
